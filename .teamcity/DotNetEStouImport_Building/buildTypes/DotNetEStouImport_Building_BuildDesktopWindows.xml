<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build Desktop (Windows)</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="bin/Clock.Desktop/win/**/*.* =&gt; bin/Clock.Desktop.zip&#xA;bin/Clock.Desktop.Uwp/win/**/*.* =&gt; bin/Clock.Desktop.Uwp.zip" />
    </options>
    <disabled-settings>
      <setting-ref ref="TRIGGER_1" />
    </disabled-settings>
    <parameters>
      <param name="system.AppxPackageDir" value="../bin/Clock.Desktop.Uwp/win/" />
      <param name="system.PublishDir" value="../bin/Clock.Desktop/win/" />
      <param name="system.VersionPrefix" value="1.0.0" />
      <param name="system.VersionSuffix" value="%build.number%" />
      <param name="system.configuration" value="Release" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="Build Desktop (Win)" type="dotnet">
        <parameters>
          <param name="command" value="msbuild" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="msbuild.version" value="MSBuild_16_Windows" />
          <param name="paths"><![CDATA[Clock.Desktop/Clock.Desktop.csproj
Clock.Desktop.Uwp/Clock.Desktop.Uwp.csproj]]></param>
          <param name="required.sdk" value="7" />
          <param name="targets" value="Restore;Rebuild;Publish" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="DotNetEStouImport_HttpsGithubComValrravnTeamcityDotnetSamplesNoktsRefsHeadsMain" />
    </vcs-settings>
    <requirements>
      <matches id="RQ_1" name="teamcity.agent.jvm.os.family" value="Windows" />
    </requirements>
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="perfmon">
        <parameters>
          <param name="teamcity.perfmon.feature.enabled" value="true" />
        </parameters>
      </extension>
    </build-extensions>
    <dependencies>
      <depend-on sourceBuildTypeId="DotNetEStouImport_Building_Build">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

