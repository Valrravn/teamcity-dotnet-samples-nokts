<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build console &amp; web (linux-x64)</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="bin =&gt; bin" />
    </options>
    <disabled-settings>
      <setting-ref ref="TRIGGER_1" />
    </disabled-settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Build console" type="dotnet">
        <parameters>
          <param name="args" value="/p:PublishTrimmed=true /p:PublishSingleFile=true" />
          <param name="command" value="publish" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="outputDir" value="bin/Clock.Console/linux-x64" />
          <param name="paths" value="Clock.Console/Clock.Console.csproj" />
          <param name="runtime" value="linux-x64" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Build web" type="dotnet">
        <parameters>
          <param name="command" value="publish" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="outputDir" value="bin/Clock.Web/linux-x64" />
          <param name="paths" value="Clock.Web/Clock.Web.csproj" />
          <param name="runtime" value="linux-x64" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="DotNetEStouImport_HttpsGithubComValrravnTeamcityDotnetSamplesNoktsRefsHeadsMain" />
    </vcs-settings>
    <requirements>
      <exists id="RQ_1" name="DotNetCoreSDK7.0_Path" />
      <exists id="RQ_2" name="DotNetCoreRuntime7.0_Path" />
    </requirements>
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="perfmon">
        <parameters>
          <param name="teamcity.perfmon.feature.enabled" value="true" />
        </parameters>
      </extension>
    </build-extensions>
    <dependencies>
      <depend-on sourceBuildTypeId="DotNetEStouImport_Building_RunTestsLinux">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

